{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "type",
    "token",
    "session"
  ],
  "properties": {
    "type": {
      "type": "string",
      "const": "credentials-based"
    },
    "token": {
      "$ref": "/correlation/access/authentication/AuthenticationToken.json"
    },
    "session": {
      "$ref": "/correlation/access/session/SimpleSession.json"
    }
  }
}